{
    "name": "rudl/cert-issuer",
    "type": "project",
    "description": "SSL Letsencrypt cert issuer",
    "keywords": ["Phore", "Dependency Injection", "Di"],
    "homepage": "https://infracamp.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Matthias Leuffen",
            "email": "matthes@leuffen.de"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-openssl": "*",
        "rudl/gitdb-lib": "*",
        "phore/filesystem": "dev-master",
        "phore/hydrator": "*",
        "phore/letsencrypt": "dev-master"
    },

    "require-dev": {
        "phore/tester": "*"
    },
    "autoload": {
        "psr-4": {
            "Rudl\\CertIssuer\\": "src"
        },
        "files": [
            "config.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
        }
    }
}
